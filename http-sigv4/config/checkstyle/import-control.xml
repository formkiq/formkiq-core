<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
    "-//Puppy Crawl//DTD Import Control 1.1//EN"
    "http://www.puppycrawl.com/dtds/import_control_1_1.dtd">

<import-control pkg="com.formkiq.module">
	
	<subpackage name="httpsigv4">
	
		<allow pkg="com.formkiq.module.http" />
	
		<allow pkg="java.io" />
		<allow pkg="java.net" />
		<allow pkg="java.net.http" />
		<allow pkg="java.time" />
		<allow pkg="java.util" />
		<allow pkg="java.nio.file" />
		
		<allow pkg="java.nio.charset" />
		<allow pkg="software.amazon.awssdk.http" />
		
		<allow pkg="software.amazon.awssdk.auth.credentials" />
		<allow pkg="software.amazon.awssdk.auth.signer" />
		<allow pkg="software.amazon.awssdk.auth.signer.params" />
		<allow pkg="software.amazon.awssdk.regions" />

	</subpackage>
	
</import-control>
