<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
    "-//Puppy Crawl//DTD Import Control 1.1//EN"
    "http://www.puppycrawl.com/dtds/import_control_1_1.dtd">

<import-control pkg="com.formkiq.plugins">
	
	<subpackage name="version">
		<allow pkg="java.util" />
		<allow pkg="software.amazon.awssdk.services.dynamodb.model" />
	</subpackage>
	
	<subpackage name="tagschema">
	
		<allow pkg="com.formkiq.aws.dynamodb.model" />
		<allow pkg="com.formkiq.validation" />
		<allow pkg="java.util" />
		
		<allow pkg="com.formkiq.module.lambdaservices" />
	</subpackage>
	
	<subpackage name="validation">
		<allow pkg="java.util" />
	</subpackage>
	
</import-control>
