<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
    "-//Puppy Crawl//DTD Import Control 1.1//EN"
    "http://www.puppycrawl.com/dtds/import_control_1_1.dtd">

<import-control pkg="com.formkiq.testutils">
	
	<subpackage name="aws">
		<allow pkg="com.google.gson" />
		<allow pkg="com.formkiq.aws.cognito" />
		<allow pkg="com.formkiq.aws.ssm" />
		<allow pkg="com.formkiq.stacks.client" />
		<allow pkg="java.io" />
		<allow pkg="java.net" />
		<allow pkg="java.util" />
		<allow pkg="software.amazon.awssdk.auth.credentials" />
		<allow pkg="software.amazon.awssdk.regions" />
		<allow pkg="software.amazon.awssdk.services.cognitoidentityprovider.model" />

	</subpackage>
	
</import-control>
