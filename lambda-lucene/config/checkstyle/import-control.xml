<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
    "-//Puppy Crawl//DTD Import Control 1.1//EN"
    "http://www.puppycrawl.com/dtds/import_control_1_1.dtd">

<import-control pkg="com.formkiq.module">
	
	<subpackage name="lambda">
	
		<subpackage name="lucene">
		
			<allow pkg="com.formkiq.module.http" />
			<allow pkg="com.amazonaws.services.lambda.runtime" />
			<allow pkg="com.formkiq.aws.dynamodb" />			
			<allow pkg="com.formkiq.graalvm.annotations" />
			<allow pkg="com.google.gson" />
			
			<allow pkg="java.io" />
			<allow pkg="java.net.http" />
			<allow pkg="java.nio" />
			<allow pkg="java.util" />
		
			<allow pkg="org.apache.lucene.codecs.lucene90" />
			<allow pkg="org.apache.lucene.store" />
			<allow pkg="org.apache.lucene.util" />
			<allow pkg="org.apache.lucene.analysis.tokenattributes" />
			<allow pkg="org.apache.lucene.codecs.lucene94" />
			<allow pkg="org.apache.lucene.codecs" />
				
			<allow pkg="com.formkiq.module.lucene" />
			<allow pkg="org.apache.lucene.document" />
			<allow pkg="org.apache.lucene.index" />
			
			<allow pkg="software.amazon.awssdk.utils" />
			<allow pkg="com.formkiq.module.typesense" />

		</subpackage>
	</subpackage>
	
</import-control>
